{
  "name": "mya-zaki/csvert",
  "description": "Import and Export CSV.",
  "keywords": [
    "csv"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "mya-zaki"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.12",
    "enorris/mbstring-stream": "^2.0",
    "symfony/var-dumper": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "MyaZaki\\Csvert\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MyaZaki\\Csvert\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "lint": "./vendor/bin/phpcs -p --standard=phpcs.xml src/",
    "fix-style": "./vendor/bin/phpcbf -p --standard=phpcs.xml src/"
  }
}
